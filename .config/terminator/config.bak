[global_config]
  title_hide_sizetext = True
  title_transmit_bg_color = "#363636"
  suppress_multiple_term_dialog = True
[keybindings]
  hide_window = ""
  group_all = ""
  group_tab = ""
  edit_window_title = ""
  edit_tab_title = ""
  edit_terminal_title = ""
[profiles]
  [[default]]
    background_darkness = 0.86
    background_type = transparent
    cursor_color = "#aaaaaa"
    font = CaskaydiaCove Nerd Font 11
    use_system_font = False
    use_theme_colors = True
    copy_on_selection = True
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
